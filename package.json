{
  "name": "@opensemanticworld/osw",
  "version": "0.0.1",
  "description": "Node package for opensemanticworld.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs, esm --dts",
    "lint": "tsc",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensemanticworld/osw-npmjs.git"
  },
  "keywords": [
    "opensemanticworld",
    "opensemanticlab",
    "osw",
    "osl"
  ],
  "author": "raederan",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/opensemanticworld/osw-npmjs/issues"
  },
  "homepage": "https://github.com/opensemanticworld/osw-npmjs#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.3",
    "@types/node": "^20.12.12",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
